 *
 * @author Gielyn Baloloy & Amyline Baccay
 */


public class MatrixApp {
    
	public static void main(String args[]){
        
	Matrix MatrixClass = new Matrix();
        
        
	Matrix matA = new Matrix();
       
	Matrix matB = new Matrix();
        
        
	System.out.println("matrix a");
        

	matA.setCell(0,0,1);
        
	matA.setCell(0,1,2);
        
	matA.setCell(0,2,3);
        
	matA.setCell(0,3,4);
        
	matA.setCell(1,0,5);
        
	matA.setCell(1,1,6);
        
	matA.setCell(1,2,7);
        
	matA.setCell(1,3,8);
        
	matA.setCell(2,0,9);
        
	matA.setCell(2,1,0);
        
	matA.setCell(2,2,1);
        
	matA.setCell(2,3,2);
        
	matA.setCell(3,0,3);
        
	matA.setCell(3,1,4);
        
	matA.setCell(3,2,5);
        
	matA.setCell(3,3,6);
        
	matA.displayMatrix();
        
        
	System.out.println("-----------------------");
        
        
	System.out.println("matrix b");
        
	matB.setCell(0,0,1);
        
	matB.setCell(0,1,2);
        
	matB.setCell(0,2,3);
        
	matB.setCell(0,3,4);
        
	matB.setCell(1,0,5);
        
	matB.setCell(1,1,6);
        
	matB.setCell(1,2,7);
        
	matB.setCell(1,3,8);
        
	matB.setCell(2,0,9);
        
	matB.setCell(2,1,0);
        
	matB.setCell(2,2,1);
        
	matB.setCell(2,3,2);
        
	matB.setCell(3,0,3);
        
	matB.setCell(3,1,4);
        
	matB.setCell(3,2,5);
        
	matB.setCell(3,3,6);
        
	matB.displayMatrix();
        
        
	System.out.println("-----------------------");
        
        
	MatrixClass.addMatrices(matA, matB);
        
	MatrixClass.displayMatrix();
    
	}
    
    

}
